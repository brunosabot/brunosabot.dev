{
  "name": "brunosabot.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "next dev",
    "lint:css": "stylelint --fix components/**/*.css styles/**/*.css",
    "lint:format": "prettier --write \"**/*.{css,js,jsx,json,md,ts,tsx}\"",
    "lint:ts": "eslint --fix \"components/**/*.tsx\" \"lib/**/*.ts\" \"pages/**/*.tsx\"",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "start": "next start -p 8080"
  },
  "dependencies": {
    "@mdi/js": "^7.1.96",
    "@notionhq/client": "^2.2.3",
    "@types/marked": "^4.0.8",
    "@types/prismjs": "^1.26.0",
    "date-fns": "^2.29.3",
    "marked": "^4.2.12",
    "next": "13.1.6",
    "next-pwa": "^5.6.0",
    "node-fetch": "^3.3.0",
    "notion-to-md": "^2.5.5",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-transition-group": "^4.4.5",
    "reading-time": "^1.5.0",
    "sha.js": "^2.4.11",
    "typescript": "4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@types/gtag.js": "^0.0.12",
    "@types/react": "18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.5",
    "@types/sha.js": "^2.4.0",
    "@types/uuid": "^9.0.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "next-sitemap": "^3.1.52",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-order": "^6.0.2",
    "stylelint-prettier": "^2.0.0"
  },
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{js,jsx}": [
      "eslint --ext .js,.ts,.tsx",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
